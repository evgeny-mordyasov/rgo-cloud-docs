<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="true">
    <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
        <Target>System.out</Target>
        <encoder>
            <pattern>
                %d{yyyy-MM-dd HH:mm:ss} %-5p %c: %m%n
            </pattern>
        </encoder>
    </appender>

    <root level="off">
        <appender-ref ref="stdout"/>
    </root>
</configuration>